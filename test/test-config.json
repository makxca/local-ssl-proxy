{
  "Proxy 1": {
    "hostname": "localhost",
    "source": 443,
    "target": 1867,
    "cert": "/etc/apache2/server.pem",
    "key": "/etc/apache2/server.key"
  },
  "Proxy 2": {
    "hostname": "localhost",
    "source": 5001,
    "target": 3001,
    "cert": "/etc/apache2/server.pem",
    "key": "/etc/apache2/server.key",
    "defaultTarget": 5000,
    "targetPath": "/api"
  }
}
